FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/fluid";
    object      setFieldTableDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

field T.water;
file "T01.csv";
readerType csv;
hasHeaderLine false;
timeColumn 0;
valueColumns (1);

function sphere;
origin (0 0 0);
radius 50e-6;
scale -1;




